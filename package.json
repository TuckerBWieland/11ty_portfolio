{
  "name": "tucker-wieland-portfolio",
  "version": "1.0.0",
  "description": "Tucker Wieland's professional portfolio website",
  "main": "index.js",
  "scripts": {
    "build": "eleventy",
    "build:css": "tailwindcss -i ./src/css/main.css -o ./_site/css/main.css",
    "build:prod": "npm run build:css && eleventy",
    "dev": "npm run build:css && eleventy --serve --watch",
    "start": "npm run dev"
  },
  "keywords": [
    "portfolio",
    "strategy",
    "operations",
    "executive",
    "business",
    "consulting"
  ],
  "author": "Tucker Wieland",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "autoprefixer": "^10.4.16",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
