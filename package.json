{
  "name": "tucker-wieland-portfolio",
  "version": "1.0.0",
  "description": "Tucker Wieland's professional portfolio website",
  "main": "index.js",
  "scripts": {
    "build": "tailwindcss -i ./src/css/main.css -o ./_site/css/main.css --watch",
    "build:css": "tailwindcss -i ./src/css/main.css -o ./_site/css/main.css",
    "build:prod": "tailwindcss -i ./src/css/main.css -o ./_site/css/main.css --minify",
    "dev": "npm run build:css && npm run build",
    "start": "npm run build:css && npm run dev"
  },
  "keywords": [
    "portfolio",
    "strategy",
    "operations",
    "executive",
    "business",
    "consulting"
  ],
  "author": "Tucker Wieland",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
